{
  "name": "travel-website-backend",
  "version": "1.1.0",
  "description": "Backend for travel website with tours, bookings, reviews",
  "type": "module",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "dev": "nodemon",
    "migrate": "npx sequelize-cli db:migrate",
    "seed": "node seeders/run-seeder.js",
    "seed:reset": "node ./seeders/reset-seeder.js",
    "setup": "npm run migrate && npm run seed"
  },
  "dependencies": {
    "@sendgrid/mail": "^8.1.6",
    "bcryptjs": "^2.4.3",
    "cloudinary": "^1.40.0",
    "compression": "^1.8.1",
    "cors": "^2.8.5",
    "dotenv": "^16.6.1",
    "express": "^4.18.2",
    "express-async-handler": "^1.2.0",
    "express-rate-limit": "^8.1.0",
    "express-validator": "^7.2.1",
    "get-stream": "^9.0.1",
    "helmet": "^8.1.0",
    "jsonwebtoken": "^9.0.2",
    "morgan": "^1.10.1",
    "multer": "^1.4.5-lts.1",
    "multer-storage-cloudinary": "^4.0.0",
    "mysql2": "^3.6.0",
    "pdfkit": "^0.14.0",
    "sequelize": "^6.32.1",
    "sharp": "^0.32.6",
    "slugify": "^1.6.6",
    "uuid": "^13.0.0",
    "winston": "^3.18.3"
  },
  "devDependencies": {
    "nodemon": "^3.1.10",
    "sequelize-cli": "6.6.1"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "keywords": [
    "travel",
    "tours",
    "backend",
    "node",
    "express",
    "sequelize",
    "mysql"
  ],
  "author": "Shekhar Kamble",
  "license": "MIT"
}
